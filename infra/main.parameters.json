{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "gei-migration-mcp"
    },
    "location": {
      "value": "eastus"
    },
    "containerImage": {
      "value": "ghcr.io/chikamsoachumsft/gei-migration-mcp:latest"
    },
    "githubSourcePat": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<subscription-id>/resourceGroups/<rg>/providers/Microsoft.KeyVault/vaults/<vault-name>"
        },
        "secretName": "github-source-pat"
      }
    },
    "githubTargetPat": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<subscription-id>/resourceGroups/<rg>/providers/Microsoft.KeyVault/vaults/<vault-name>"
        },
        "secretName": "github-target-pat"
      }
    },
    "adoPat": {
      "value": ""
    }
  }
}
